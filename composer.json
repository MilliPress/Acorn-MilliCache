{
  "name": "millipress/acorn-millicache",
  "description": "Stores Acorn route responses in MilliCache's Redis cache",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Philipp Wellmer",
      "homepage": "https://millipress.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "roots/acorn": "^4.0 || ^5.0",
    "millipress/millicache": "^1.0.2"
  },
  "suggest": {
    "millipress/acorn-millirules": "Add route-aware rules, conditions & actions for Acorn routes and cache them"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "phpstan/phpstan": "^2.0",
    "pestphp/pest": "^4.0",
    "orchestra/testbench": "^10.9"
  },
  "autoload": {
    "psr-4": {
      "MilliCache\\Acorn\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MilliCache\\Acorn\\Tests\\": "tests/"
    }
  },
  "extra": {
    "acorn": {
      "providers": [
        "MilliCache\\Acorn\\ServiceProvider"
      ]
    }
  },
  "scripts": {
    "lint": "pint --test",
    "lint:fix": "pint",
    "phpstan": "phpstan analyze",
    "test": "pest"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
