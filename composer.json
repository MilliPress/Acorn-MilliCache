{
  "name": "millipress/acorn-millicache",
  "description": "Stores Acorn route responses in MilliCache's Redis cache",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Philipp Wellmer",
      "homepage": "https://millipress.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "roots/acorn": "^4.0 || ^5.0",
    "millipress/acorn-millirules": "dev-main",
    "millipress/millicache": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "MilliPress\\AcornMilliCache\\": "src/"
    }
  },
  "extra": {
    "acorn": {
      "providers": [
        "MilliPress\\AcornMilliCache\\AcornMilliCacheServiceProvider"
      ]
    }
  },
  "scripts": {
    "lint": "pint --test",
    "lint:fix": "pint"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
